<CustomDrawerItem@MDNavigationDrawerItem>
    text_color: 0, 0, 0, 1
    icon_color: 0, 0, 0, 1
<CustomTextField>
    icon_left_color: 0, 0, 0, 1
    icon_left_color_normal: 0, 0, 0, 1
    icon_left_color_focus: 0, 0, 0, 1
<TableHeader@MDLabel>
    bold: True
    text_color: 0, 0, 0, 1
    halign: "left"
<CustomButton@MDRaisedButton>
    md_bg_color: 0, 0, 0, 1
<TransactionSummary>
    orientation: "horizontal"
    size_hint: 1, None
    height: self.minimum_height
    spacing: 20
    padding: 20, 20
    GridLayout:
        size_hint: .4, None
        spacing: 20
        cols: 1
        MDLabel:
            text: "Purchased by"
        MDLabel:
            text: "Direction"
        MDLabel:
            text: "Departure Date"
        MDLabel:
            text: "Passenger"
        MDLabel:
            text: "Seat No"
        MDLabel:
            text: "Unit Price"
        MDLabel:
            text: "Total Payment"
        MDLabel:
            text: "Status"
    GridLayout:
        size_hint: .6, None
        spacing: 20
        cols: 1
        MDLabel:
            id: username_fld
            text: root.username
        MDLabel:
            id: destination_fld
            text: root.destination
        MDLabel:
            id: departure_date_fld
            text: root.departure_date
        MDLabel:
            id: passenger_fld
            text: root.passenger
        MDLabel:
            id: seat_no_fld
            text: root.seat_no
        MDLabel:
            id: unit_price_fld
            text: "USD " + root.unit_price
        MDLabel:
            id: total_payment_fld
            text: "USD " + root.total_payment
        MDLabel:
            id: paid_status_fld
            text: root.paid_status
<NoData>
    MDIcon:
        icon: root.icon
        font_size: "64sp"
        pos_hint: {'center_x': .5, 'center_y': .55}
    MDLabel:
        text: root.text
        halign: "center"
        font_style: "H2"
        pos_hint: {'center_x': .5, 'center_y': .45}
<Transaction>
    radius: [6, 6, 6, 6]
    style: "elevated"
    elevation: 6
    padding: 100, 20
    size_hint_y: None
    GridLayout:
        cols: 3
        spacing: 20
        MDLabel:
            text: "Destination"
        MDLabel:
            text: "Booking Date"
        MDLabel:
            text: "Price"
        MDLabel:
            text: root.destination
        MDLabel:
            text: root.booking_date
        MDLabel:
            text: "USD " + root.price
        MDBoxLayout:
            orientation: "horizontal"
            size_hint_y: None
            height: self.minimum_height
            MDIcon:
                icon: "bus"
            MDLabel:
                text: root.bus_name
        MDBoxLayout:
            orientation: "horizontal"
            size_hint_y: None
            height: self.minimum_height
            MDIcon:
                icon: "car-seat"
            MDLabel:
                text: "Seat:" + root.seat
        MDLabel:
            bold: True
            text: root.paid_status
<BusStatusField>
    orientation: "horizontal"
    spacing: 20
    size_hint_y: None
    height: textfield.height
    CustomTextField:
        id: textfield
        hint_text: "Status"
        disabled: True
    CustomButton:
        text: "Active"
        on_release:
            textfield.text = self.text
            root.text = self.text
    CustomButton:
        text: "Inactive"
        on_release:
            textfield.text = self.text
            root.text = self.text
<BusTypeField>
    orientation: "horizontal"
    spacing: 20
    size_hint_y: None
    height: textfield.height
    CustomTextField:
        id: textfield
        hint_text: "Bus Type"
        disabled: True
    CustomButton:
        text: "Express"
        on_release:
            textfield.text = self.text
            root.text = self.text
    CustomButton:
        text: "VIP"
        on_release:
            textfield.text = self.text
            root.text = self.text
<UserTypeField>:
    orientation: "horizontal"
    spacing: 20
    size_hint_y: None
    height: textfield.height
    CustomTextField:
        id: textfield
        hint_text: "User Role"
        disabled: True
    CustomButton:
        text: "Admin"
        on_release:
            textfield.text = self.text
            root.text = self.text
    CustomButton:
        text: "User"
        on_release:
            textfield.text = self.text
            root.text = self.text
<AdminWindow>
    id: main_win
    orientation: "vertical"
    MDNavigationLayout:
        MDNavigationDrawer:
            id: nav_drawer
            type: "standard"
            anchor: "left"
            MDNavigationDrawerMenu:
                spacing: 20
                MDNavigationDrawerHeader:
                    id: nav_drawer_header
                    title: "Settings"
                    title_color: 0, 0, 0, 1
                    text: "Account Name"
                    text_color: 0, 0, 0, 1
                MDNavigationDrawerLabel:
                    text: "Manage User"
                CustomDrawerItem:
                    icon: "account-plus"
                    text: "Add User"
                    on_release:
                        nav_drawer.set_state("close")
                        root.goto_add_user()
                CustomDrawerItem:
                    icon: "account"
                    text: "Update User"
                    on_release:
                        nav_drawer.set_state("close")
                        root.goto_update_user()
                CustomDrawerItem:
                    icon: "account-remove"
                    text: "Remove User"
                    on_release:
                        nav_drawer.set_state("close")
                        root.goto_remove_user()
                MDNavigationDrawerDivider:
                    color: 0, 0, 0, .5
                MDNavigationDrawerLabel:
                    text: "Manage Trip"
                CustomDrawerItem:
                    icon: "ticket"
                    text: "Add Trip"
                    on_release:
                        nav_drawer.set_state("close")
                        root.goto_add_trip()
                CustomDrawerItem:
                    icon: "ticket-confirmation"
                    text: "Update Trip"
                    on_release:
                        nav_drawer.set_state("close")
                        root.goto_update_trip()
                MDNavigationDrawerDivider:
                    color: 0, 0, 0, .5
                MDNavigationDrawerLabel:
                    text: "Manage Bus"
                CustomDrawerItem:
                    icon: "bus"
                    text: "Show Bus"
                    on_release:
                        nav_drawer.set_state("close")
                        root.goto_bus()
                CustomDrawerItem:
                    icon: "bus-multiple"
                    text: "Add Bus"
                    on_release:
                        nav_drawer.set_state("close")
                        root.goto_add_bus()
                CustomDrawerItem:
                    icon: "bus-alert"
                    text: "Update Bus"
                    on_release:
                        nav_drawer.set_state("close")
                        root.goto_update_bus()
                MDNavigationDrawerDivider:
                    color: 0, 0, 0, .5
                MDNavigationDrawerLabel:
                    text: "Transaction"
                CustomDrawerItem:
                    icon: "file-table-box-multiple"
                    text: "Transaction Detail"
                    on_release:
                        nav_drawer.set_state("close")
                        root.goto_transaction()
                MDNavigationDrawerDivider:
                    color: 0, 0, 0, .5
                MDNavigationDrawerLabel:
                    text: "Others"
                    bold: True
                CustomDrawerItem:
                    icon: "account"
                    text: "Edit Profile"
                    on_release:
                        root.goto_edit_profile()
                        nav_drawer.set_state("close")
                CustomDrawerItem:
                    icon: "decagram-outline"
                    text: "Change Password"
                    on_release:
                        root.goto_settings()
                        nav_drawer.set_state("close")
                CustomDrawerItem:
                    icon: "logout"
                    text: "Logout"
                    on_release:
                        root.show_logout_dialog()
                        nav_drawer.set_state("close")
                MDNavigationDrawerDivider:
                    color: 0, 0, 0, .5
        ScreenManager:
            id: scrn_mngr
            Screen:
                id: main_scrn
                name: "main_scrn"
                MDBoxLayout:
                    orientation: "vertical"
                    MDTopAppBar:
                        id: toolbar
                        title: "Admin Dashboard"
                        md_bg_color: 0, 0, 0, 1
                        left_action_items:
                            [ \
                            [ \
                            'menu', lambda x: \
                            nav_drawer.set_state("open") if nav_drawer.state == "close" \
                            else nav_drawer.set_state("close") \
                            ] \
                            ]
                    MDBottomNavigation:
                        first_widget: "manage_user"
                        selected_color_background: 0, 0, 0, 1
                        text_color_active: 0, 0, 0, 1
                        MDBottomNavigationItem:
                            id: manage_user
                            name: "manage_user"
                            text: "Manage Users"
                            icon: "account"
                            on_tab_release:
                                root.show_user_table()
                            MDBoxLayout:
                                orientation: "vertical"
                                padding: 20, 20
                                spacing: 20
                                canvas.before:
                                    Color:
                                        rgba: 0, 0, 0, .25
                                    Rectangle:
                                        size: self.size
                                        pos: self.pos
                                MDBoxLayout:
                                    orientation: "vertical"
                                    padding: 20, 10
                                    spacing: 10
                                    canvas.before:
                                        RoundedRectangle:
                                            size: self.size
                                            pos: self.pos
                                            radius: [12, 12, 12 ,12]
                                    GridLayout:
                                        cols: 10
                                        size_hint_y: .1
                                        spacing: 10
                                        TableHeader:
                                            text: "ID"
                                        TableHeader:
                                            text: "Username"
                                        TableHeader:
                                            text: "Password"
                                        TableHeader:
                                            text: "First Name"
                                        TableHeader:
                                            text: "Last Name"
                                        TableHeader:
                                            text: "Date of Birth"
                                        TableHeader:
                                            text: "Email"
                                        TableHeader:
                                            text: "Phone"
                                        TableHeader:
                                            text: "Role"
                                        TableHeader:
                                            text: "Active"
                                    ScrollView:
                                        GridLayout:
                                            id: user_table_content
                                            cols: 10
                                            spacing: 10
                                            size_hint_y: None
                                MDBoxLayout:
                                    orientation: "horizontal"
                                    spacing: 20
                                    size_hint_y: None
                                    height: btn_add_user.height
                                    CustomButton:
                                        id: btn_add_user
                                        text: "Add User"
                                        size_hint_x: .3
                                        on_release:
                                            root.goto_add_user()
                                    CustomButton:
                                        text: "Update User"
                                        size_hint_x: .3
                                        on_release:
                                            root.goto_update_user()
                                    CustomButton:
                                        text: "Remove User"
                                        size_hint_x: .3
                                        on_release:
                                            root.goto_remove_user()
                        MDBottomNavigationItem:
                            id: manage_ticket
                            name: "manage_trip"
                            text: "Manage Trip"
                            icon: "bus"
                            on_tab_release:
                                root.show_trip_table()
                            MDBoxLayout:
                                orientation: "vertical"
                                padding: 20, 20
                                spacing: 20
                                canvas.before:
                                    Color:
                                        rgba: 0, 0, 0, .25
                                    Rectangle:
                                        size: self.size
                                        pos: self.pos
                                MDBoxLayout:
                                    orientation: "vertical"
                                    padding: 20, 10
                                    spacing: 10
                                    canvas.before:
                                        RoundedRectangle:
                                            size: self.size
                                            pos: self.pos
                                            radius: [12, 12, 12 ,12]
                                    GridLayout:
                                        cols: 8
                                        size_hint_y: .1
                                        spacing: 10
                                        TableHeader:
                                            text: "ID"
                                        TableHeader:
                                            text: "Bus"
                                        TableHeader:
                                            text: "Destination"
                                        TableHeader:
                                            text: "Price"
                                        TableHeader:
                                            text: "Available Seat"
                                        TableHeader:
                                            text: "Departure Date"
                                        TableHeader:
                                            text: "Departure Time"
                                        TableHeader:
                                            text: "Status"
                                    ScrollView:
                                        GridLayout:
                                            id: trip_table_content
                                            cols: 8
                                            size_hint_y: None
                                            spacing: 10
                                MDBoxLayout:
                                    orientation: "horizontal"
                                    spacing: 20
                                    size_hint_y: None
                                    height: btn_add_trip.height
                                    CustomButton:
                                        id: btn_add_trip
                                        text: "Add Trip"
                                        size_hint_x: .5
                                        on_release:
                                            root.goto_add_trip()
                                    CustomButton:
                                        text: "Update Trip"
                                        size_hint_x: .5
                                        on_release:
                                            root.goto_update_trip()
            Screen:
                id: scrn_edit_profile
                name: "scrn_edit_profile"
                MDBoxLayout:
                    orientation: "vertical"
                    MDTopAppBar:
                        title: "Edit Profile"
                        md_bg_color: 0, 0, 0, 1
                        right_action_items: [['arrow-left-bold', lambda x: root.goto_main_screen()]]
                    MDBoxLayout:
                        id: edit_profile_opt
                        orientation: "vertical"
                        padding: main_win.size[0]/4, main_win.size[1]/4
                        spacing: 20
                        canvas.before:
                            Color:
                                rgba: 0, 0, 0, .25
                            Rectangle:
                                size: self.size
                                pos: self.pos
                        CustomTextField:
                            id: first_name_fld
                            hint_text: "First Name"
                            focus: True
                            on_text_validate:
                                last_name_fld.focus = True
                        CustomTextField:
                            id: last_name_fld
                            hint_text: "Last Name"
                            on_text_validate:
                                phone_fld.focus = True
                        CustomTextField:
                            id: phone_fld
                            hint_text: "Phone Number"
                            on_text_validate:
                                email_fld.focus = True
                        CustomTextField:
                            id: email_fld
                            hint_text: "Email"
                            helper_text: "Invalid Email Address"
                            helper_text_mode: "on_error"
                            on_text: self.error = False
                        CustomTextField:
                            id: dob_fld
                            hint_text: "Date of Birth(DD/MM/YYYY)"
                            on_focus:
                                root.dob_date_picker()
                        CustomButton:
                            text: "Update"
                            padding: main_win.size[0]/3, 0
                            size_hint_x: 1
                            on_release:
                                root.update_info(first_name_fld.text, last_name_fld.text,
                                phone_fld.text, email_fld.text, dob_fld.text)
            Screen:
                id: scrn_settings
                name: "scrn_settings"
                MDBoxLayout:
                    orientation: "vertical"
                    MDTopAppBar:
                        title: "Change Password"
                        md_bg_color: 0, 0, 0, 1
                        right_action_items: [['arrow-left-bold', lambda x: root.goto_main_screen()]]
                    MDBoxLayout:
                        id: settings_opt
                        orientation: "vertical"
                        padding: main_win.size[0]/4, main_win.size[1]/4
                        spacing: 20
                        canvas.before:
                            Color:
                                rgba: 0, 0, 0, .25
                            Rectangle:
                                size: self.size
                                pos: self.pos
                        CustomTextField:
                            id: old_pass
                            hint_text: "Old Password"
                            helper_text: "Incorrect Password"
                            helper_text_mode: "on_error"
                            icon_left: "key-variant"
                            password: True
                            on_text:
                                self.error = False
                            on_text_validate:
                                new_pass.focus = True
                        CustomTextField:
                            id: new_pass
                            hint_text: "New Password"
                            helper_text: "Invalid Password"
                            helper_text_mode: "on_error"
                            icon_left: "key-variant"
                            password: True
                            on_text:
                                self.error = False
                            on_text_validate:
                                confirm_pass.focus = True
                        CustomTextField:
                            id: confirm_pass
                            hint_text: "Confirm New Password"
                            helper_text: "Confirm password does not match"
                            helper_text_mode: "on_error"
                            icon_left: "key-variant"
                            password: True
                            on_text:
                                self.error = False
                            on_text_validate:
                                root.update_password(old_pass.text, new_pass.text, confirm_pass.text)
                        MDBoxLayout:
                            orientation: "horizontal"
                            spacing: 20
                            CustomButton:
                                text: "Update"
                                size_hint_x: .5
                                on_release:
                                    root.update_password(old_pass.text, new_pass.text, confirm_pass.text)
                            CustomButton:
                                text: "Show Passwords"
                                size_hint_x: .5
                                on_release:
                                    old_pass.password = False if old_pass.password == True else True
                                    new_pass.password = False if new_pass.password == True else True
                                    confirm_pass.password = False if confirm_pass.password == True else True
            Screen:
                id: scrn_add_user
                name: 'scrn_add_user'
                MDBoxLayout:
                    orientation: "vertical"
                    canvas.before:
                        Color:
                            rgba: 0, 0, 0, .25
                        Rectangle:
                            size: self.size
                            pos: self.pos
                    MDTopAppBar:
                        title: "Add User"
                        md_bg_color: 0, 0, 0, 1
                        right_action_items:
                            [ \
                            ['arrow-left-bold', lambda x: root.goto_main_screen()] \
                            ]
                    MDBoxLayout:
                        orientation: "vertical"
                        padding: main_win.size[0]/4, main_win.size[1]/3
                        spacing: 20
                        CustomTextField:
                            id: add_username_fld
                            hint_text: "Username"
                            helper_text: "Username already taken"
                            helper_text_mode: "on_error"
                            focus: True
                            on_text:
                                self.error = False
                            on_text_validate:
                                add_password_fld.focus = True
                        CustomTextField:
                            id: add_password_fld
                            hint_text: "Password"
                            helper_text: "Invalid Password"
                            helper_text_mode: "on_error"
                            password: True
                            on_text:
                                self.error = False
                            on_text_validate:
                                add_email_fld.focus = True
                        CustomTextField:
                            id: add_email_fld
                            hint_text: "Email"
                            helper_text: "This Email has already been used"
                            helper_text_mode: "on_error"
                            on_text:
                                self.error = False
                            on_text_validate:
                                self.focus = False
                        CustomButton:
                            text: "Add User"
                            on_release:
                                root.add_user(add_username_fld.text, add_password_fld.text, add_email_fld.text)
            Screen:
                id: scrn_update_user
                name: 'scrn_update_user'
                MDBoxLayout:
                    orientation: "vertical"
                    canvas.before:
                        Color:
                            rgba: 0, 0, 0, .25
                        Rectangle:
                            size: self.size
                            pos: self.pos
                    MDTopAppBar:
                        title: "Update User"
                        md_bg_color: 0, 0, 0, 1
                        right_action_items:
                            [ \
                            ['arrow-left-bold', lambda x: root.goto_main_screen()] \
                            ]
                    MDBoxLayout:
                        orientation: "vertical"
                        padding: main_win.size[0]/4, main_win.size[1]/3
                        spacing: 20
                        CustomTextField:
                            id: update_username_fld
                            hint_text: "Username"
                            helper_text: "Invalid Username"
                            helper_text_mode: "on_error"
                            focus: True
                            on_text: self.error = False
                            on_text_validate:
                                update_password_fld.focus = True
                        CustomTextField:
                            id: update_password_fld
                            password: True
                            hint_text: "New Password"
                            helper_text: "Invalid Password"
                            helper_text_mode: "on_error"
                            on_text: self.error = False
                            on_text_validate:
                                update_email_fld.focus = True
                        CustomTextField:
                            id: update_email_fld
                            hint_text: "Set Email"
                            helper_text: "Invalid Email Address"
                            helper_text_mode: "on_error"
                            on_text: self.error = False
                            on_text_validate:
                                update_phone_fld.focus = True
                        CustomTextField:
                            id: update_phone_fld
                            input_filter: "int"
                            hint_text: "Set Phone Number"
                        UserTypeField:
                            id: update_role_fld
                        CustomButton:
                            text: "Update"
                            size_hint_y: 1
                            on_release:
                                root.update_user(update_username_fld.text, update_password_fld.text,
                                update_email_fld.text, update_phone_fld.text, update_role_fld.text)

            Screen:
                id: scrn_remove_user
                name: 'scrn_remove_user'
                MDBoxLayout:
                    orientation: "vertical"
                    canvas.before:
                        Color:
                            rgba: 0, 0, 0, .25
                        Rectangle:
                            size: self.size
                            pos: self.pos
                    MDTopAppBar:
                        title: "Remove User"
                        md_bg_color: 0, 0, 0, 1
                        right_action_items:
                            [ \
                            ['arrow-left-bold', lambda x: root.goto_main_screen()] \
                            ]
                    MDBoxLayout:
                        orientation: "vertical"
                        spacing: 20
                        padding: main_win.size[0]/4, main_win.size[1]/3
                        CustomTextField:
                            id: remove_username_fld
                            hint_text: "Username"
                            focus: True
                            helper_text: "Username does not exists!"
                            helper_text_mode: "on_error"
                            on_text: self.error = False
                            on_text_validate:
                                root.remove_user_dialog(remove_username_fld.text)
                        CustomButton:
                            text: "Remove User"
                            on_release:
                                root.remove_user_dialog(remove_username_fld.text)
            Screen:
                id: scrn_add_trip
                name: 'scrn_add_trip'
                MDBoxLayout:
                    orientation: "vertical"
                    canvas.before:
                        Color:
                            rgba: 0, 0, 0, .25
                        Rectangle:
                            size: self.size
                            pos: self.pos
                    MDTopAppBar:
                        title: "Add Trip"
                        md_bg_color: 0, 0, 0, 1
                        right_action_items:
                            [ \
                            ['arrow-left-bold', lambda x: root.goto_main_screen()] \
                            ]
                    MDBoxLayout:
                        orientation: "vertical"
                        padding: main_win.size[0]/4, main_win.size[1]/3
                        spacing: 20
                        MDCard:
                            orientation: "vertical"
                            radius: [12, 12, 12, 12]
                            padding: 20, 20
                            MDBoxLayout:
                                orientation: "horizontal"
                                spacing: 20
                                size_hint_y: None
                                height: add_trip_location_fld.height
                                CustomTextField:
                                    id: add_trip_location_fld
                                    hint_text: "Choose Location"
                                MDIconButton:
                                    icon: "arrow-down-drop-circle-outline"
                                    on_release:
                                        root.open_location_menu()
                            MDBoxLayout:
                                orientation: "horizontal"
                                spacing: 20
                                size_hint_y: None
                                height: add_trip_bus_fld.height
                                CustomTextField:
                                    id: add_trip_bus_fld
                                    hint_text: "Choose Bus"
                                MDIconButton:
                                    icon: "arrow-down-drop-circle-outline"
                                    on_release:
                                        root.open_bus_menu()
                            MDBoxLayout:
                                orientation: "horizontal"
                                spacing: 20
                                size_hint_y: None
                                height: add_trip_departure_date_fld.height
                                TwoLineListItem:
                                    id: add_trip_departure_date_fld
                                    text: "Departure Date"
                                    secondary_text: "Add Departure Date"
                                    on_release:
                                        root.departure_date_picker()
                                MDBoxLayout:
                                    orientation: "horizontal"
                                    spacing: 20
                                    size_hint_y: None
                                    height: add_trip_departure_time_fld.height
                                    CustomTextField:
                                        id: add_trip_departure_time_fld
                                        hint_text: "Choose Departure Time"
                                    MDIconButton:
                                        icon: "arrow-down-drop-circle-outline"
                                        on_release:
                                            root.open_departure_time_menu()
                            CustomButton:
                                text: "Add Trip"
                                size_hint_x: 1
                                on_release:
                                    root.add_trip(add_trip_location_fld.text, add_trip_bus_fld.text,
                                    add_trip_departure_date_fld.secondary_text, add_trip_departure_time_fld.text)
            Screen:
                id: scrn_update_trip
                name: 'scrn_update_trip'
                MDBoxLayout:
                    orientation: "vertical"
                    canvas.before:
                        Color:
                            rgba: 0, 0, 0, .25
                        Rectangle:
                            size: self.size
                            pos: self.pos
                    MDTopAppBar:
                        title: "Update Trip"
                        md_bg_color: 0, 0, 0, 1
                        right_action_items:
                            [ \
                            ['arrow-left-bold', lambda x: root.goto_main_screen()] \
                            ]
